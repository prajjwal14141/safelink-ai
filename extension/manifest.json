{
  "manifest_version": 3,
  "name": "SafeLink AI Protector",
  "version": "1.0",
  "description": "Uses a custom AI to block malicious URLs in real-time.",
  "permissions": [
    "tabs", 
    "storage"
  ],
  "host_permissions": [
    "http://127.0.0.1:5000/analyze"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "SafeLink AI"
  }
}

